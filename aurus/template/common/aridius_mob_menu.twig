{% if (theme_aurus_mob_logo != 1) %} 
<div class="mmune_logo">
   {% if (logo) %} 
   {% if (location_mmenu == 'common-home') %} 
   <img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-responsive" />
   {% else %} 
   <a href="{{ home }}"><img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-responsive" /></a>
   {% endif %} 
   {% else %} 
   <h1><a href="{{ home }}">{{ name }}</a></h1>
   {% endif %} 
</div>
{% endif %}
{% if (theme_aurus_mob_home != 1 and location_mmenu != 'common-home') %} 
<ul class="nav navbar-nav">
   <li class = "home_mob"><a href="{{ base }}">{{ text_home }}</a></li>
</ul>
{% endif %} 
{% if (theme_aurus_mobcat_group != 1) %} 
<button type="button" class="navbar-toggle menu_up collapsed" data-toggle="collapse" data-target="#navbar_mmenu"><span class="cat_mob"><i class="fa fa-bars"></i> {{ text_category }}</span><i class="fammenu fa fa-plus"></i></button>
{% endif %} 
<span class="clearfix"></span>
<div {% if (theme_aurus_mobcat_group != 1) %} class="navbar-collapse collapse" {% endif %} id="navbar_mmenu">
   <div id="menu" class="navbar">
      <div class="menu_mob_plus">
         <div class="hidem">
            <ul class="nav navbar-nav">
               {% for category in categories %} 
               {% if (category['children']) %} 
               <li class="plus"><i class="fa fa-plus"></i><i class="fa fa-minus"></i></li>
               <li class="{% if (category['category_id'] == category_id) %}{{ 'activetopmenu' }}{% endif %} dropdown">
                  <a class="with-child dfgfd dropdown-toggle" href="{{ category['href'] }}" data-hover="dropdown" data-delay="1" data-close-others="false">{% if (category['image_menu_icol']) %}<span class="menumob_icon_svg"><img src="{{ category['image_menu_icol'] }}" alt="{{ category['name'] }}" title="{{ category['name'] }}"></span>{% endif %}  {% if (category['image_menu_svg']) %}<span class="menumob_icon_svg">{{ category['image_menu_svg'] }}</span>{% endif %} {{ category['name'] }}{% if (aurus_sticker_menu != 1) %}{% if (category['stickers'] == '1') %}<span class="stickersmenu1">{{ theme_aurus_name_sticker_menu_new[language_id] }}</span>{% endif %}{% if (category['stickers'] == '2') %}<span class="stickersmenu2">{{ theme_aurus_name_sticker_menu_sale[language_id] }}</span>{% endif %}{% if (category['stickers'] == '3') %}<span class="stickersmenu3">{{ theme_aurus_name_sticker_menu_top[language_id] }}</span>{% endif %}{% endif %}<span class="fa fa-angle-down menu"></span></a>
				  <div class="dropdown-menu">
                     <div class="dropdown-inner children-category">
                        {% for child in category['children'] %} 
                        <ul class="list-unstyled">
                           {% if (child['children_lv3'] is defined and child['children_lv3']) %} 
                           <li class="with-child" >
                              <a href="{{ child['href'] }}"><span class="style2lv">{{ child['name'] }}{% if (theme_aurus_sticker_menu != 1) %}{% if (child['stickers'] == '1') %}<span class="stickersmenu1">{{ theme_aurus_name_sticker_menu_new[language_id] }}</span>{% endif %}{% if (child['stickers'] == '2') %}<span class="stickersmenu2">{{ theme_aurus_name_sticker_menu_sale[language_id] }}</span>{% endif %}{% if (child['stickers'] == '3') %}<span class="stickersmenu3">{{ theme_aurus_name_sticker_menu_top[language_id] }}</span>{% endif %}{% endif %}</span></a>
                           </li>
                           {% if (theme_aurus_3lv_menu != 1) %}         
                           {% for child_lv3 in child['children_lv3'] %} 
                           <li class="children_lv3"><a href="{{ child_lv3['href'] }}">&nbsp;&nbsp;- {{ child_lv3['name'] }}</a></li>
                           {% endfor %} 
                           {% endif %}	
                           {% else %} 
                           <li><a href="{{ child['href'] }}"><span class="style2lv">{{ child['name'] }}{% if (theme_aurus_sticker_menu != 1) %}{% if (child['stickers'] == '1') %}<span class="stickersmenu1">{{ theme_aurus_name_sticker_menu_new[language_id] }}</span>{% endif %}{% if (child['stickers'] == '2') %}<span class="stickersmenu2">{{ theme_aurus_name_sticker_menu_sale[language_id] }}</span>{% endif %}{% if (child['stickers'] == '3') %}<span class="stickersmenu3">{{ theme_aurus_name_sticker_menu_top[language_id] }}</span>{% endif %}{% endif %}</span></a></li>
                           {% endif %} 
                        </ul>
                        {% endfor %} 
                     </div>
                  </div>
               </li>
               {% else %} 
               <li class="{% if (category['category_id'] == category_id) %}{{ 'activetopmenu' }}{% endif %}"><a href="{{ category['href'] }}">{% if (category['image_menu_icol']) %}<span class="menumob_icon_svg"><img src="{{ category['image_menu_icol'] }}" alt="{{ category['name'] }}" title="{{ category['name'] }}"></span>{% endif %}  {% if (category['image_menu_svg']) %}<span class="menumob_icon_svg">{{ category['image_menu_svg'] }}</span>{% endif %} {{ category['name'] }}{% if (aurus_sticker_menu != 1) %}{% if (category['stickers'] == '1') %}<span class="stickersmenu1">{{ theme_aurus_name_sticker_menu_new[language_id] }}</span>{% endif %}{% if (category['stickers'] == '2') %}<span class="stickersmenu2">{{ theme_aurus_name_sticker_menu_sale[language_id] }}</span>{% endif %}{% if (category['stickers'] == '3') %}<span class="stickersmenu3">{{ theme_aurus_name_sticker_menu_top[language_id] }}</span>{% endif %}{% endif %}</a></li>
			   {% endif %} 
               {% endfor %} 
            </ul>
         </div>
         <ul class="nav navbar-nav">
            <!--other link-->
            {% if (theme_aurus_main_link_menu[language_id]) %} 
            <li class="plus"><i class="fa fa-plus"></i><i class="fa fa-minus"></i></li>
            <li class="dropdown">
               <a class="with-child dropdown-toggle" href="{{ theme_aurus_main_link_href_menu[language_id] }}" data-hover="dropdown" data-delay="1" data-close-others="false">{{ theme_aurus_main_link_menu[language_id] }}<span class="fa fa-angle-down menu"></span></a>
               <div class="dropdown-menu">
                  <div class="dropdown-inner1 children-category">
                     <ul class="list-unstyled">
                        {% if (theme_aurus_top_links4 ) %} 
                        {% for theme_aurus_top_link4 in theme_aurus_top_links4 %} 
                        <li>
                           <a {% if (theme_aurus_top_link4['link_top'][language_id]) %} href="{{ theme_aurus_top_link4['link_top'][language_id] }}"{% endif %} title="{{ theme_aurus_top_link4['title'][language_id] }}">{% if (theme_aurus_top_link4['faicons_top']) %} <i class="{{ theme_aurus_top_link4['faicons_top'] }}"></i>{% endif %}{% if (theme_aurus_top_link4['title']) %} {{ theme_aurus_top_link4['title'][language_id] }}{% endif %}</a>
                        </li>
                        {% endfor %} 
                        {% endif %} 
                     </ul>
                  </div>
               </div>
            </li>
            {% endif %} 
            <!--other link END-->
            <!--informations-->
            {% if (theme_aurus_info_menu != 1) %} 
            {% if (informations) %} 
            <li class="plus"><i class="fa fa-plus"></i><i class="fa fa-minus"></i></li>
            <li class="dropdown">
               <a class="with-child dropdown-toggle" data-hover="dropdown" data-delay="1" data-close-others="false">{{ text_information }}<span class="fa fa-angle-down menu"></span></a>
               <div class="dropdown-menu">
                  <div class="dropdown-inner1 children-category">
                     <ul class="list-unstyled">
                        {% for information in informations %} 
                        <li><a href="{{ information['href'] }}">{{ information['title'] }}</a></li>
                        {% endfor %} 
                     </ul>
                  </div>
               </div>
            </li>
            {% endif %} 
            {% endif %} 
            <!--informations END-->
         </ul>
      </div>
   </div>
</div>
<div class="menu_mob_plus">
   <ul class="nav navbar-nav">
      <!--add menu link-->
      {% if (theme_aurus_top_links8 ) %} 
      {% for theme_aurus_top_link8 in theme_aurus_top_links8 %} 
      <li>
         <a {% if (theme_aurus_top_link8['link_top'][language_id]) %} href="{{ theme_aurus_top_link8['link_top'][language_id] }}"{% endif %}>{% if (theme_aurus_top_link8['faicons_top']) %} <i class="{{ theme_aurus_top_link8['faicons_top'] }}"></i> {% endif %}{% if (theme_aurus_top_link8['title']) %}{{ theme_aurus_top_link8['title'][language_id] }}{% endif %}</a>
      </li>
      {% endfor %} 
      {% endif %} 
      <!--add menu link-->
