      <script><!--
         function minus(){
         if(document.getElementsByClassName ("input-quantity")[0].value <= 1) {
         return false;
         }
         document.getElementsByClassName ("input-quantity")[0].value --;
         };
         function plus(){
         document.getElementsByClassName ("input-quantity")[0].value ++;
         };
         $('.minus').click(function(){
         minus();
         });
         $('.plus').click(function(){
         plus();
         });
         //-->
