                           <li>{% if (sku) %} {{ aurus_product_sku[language_id] }} {% endif %} {{ sku }}</li>
                           {% endif %} 
                           {% if (aurus_product_show_upc != 1) %} 
                           <li>{% if (upc) %} {{ aurus_product_upc[language_id] }} {% endif %} {{ upc }}</li>
                           {% endif %} 
                           {% if (aurus_product_show_ean != 1) %} 
                           <li>{% if (ean) %} {{ aurus_product_ean[language_id] }} {% endif %} {{ ean }}</li>
                           {% endif %} 
                           {% if (aurus_product_show_jan != 1) %} 
                           <li>{% if (jan) %} {{ aurus_product_jan[language_id] }} {% endif %} {{ jan }}</li>
                           {% endif %} 
                           {% if (aurus_product_show_isbn != 1) %} 
                           <li>{% if (isbn) %} {{ aurus_product_isbn[language_id] }} {% endif %} {{ isbn }}</li>
                           {% endif %} 
                           {% if (aurus_product_show_mpn != 1) %} 
                           <li>{% if (mpn) %} {{ aurus_product_mpn[language_id] }} {% endif %} {{ mpn }}</li>
