                           {% if (aurus_product_man_img != 1) %} 
                           <li><a href="{{ manufacturers }}">
                              {% if ( manufacturer_img is defined ) %} 
                              <img  class="pull-right hidden-sm" src="{{ manufacturer_img }}" alt="{{ manufacturer }}" title="{{ manufacturer }}">
