                        {% if (stickers2) %} 
                        {% for sticker2 in stickers2 %} 
                        <div class="stickers-all" style="color:{{ sticker2['color'] }};background:{{ sticker2['background'] }};">{{ sticker2['name'] }}</div>
                        {% endfor %} 
                        {% endif %} 
                     </div>
                     <ul class="swiper-wrapper">
                        {% if (thumb) %} 
                        <li class="swiper-slide"><span class="thumbnail imglink gallery_img_main">
                           <img id="main-image" class="main-image" itemprop="image" src="{{ thumb }}" title="{{ heading_title }}" alt="{{ heading_title }}" data-zoom-image="{{ popup }}" /></span>
                        </li>
                        {% endif %} 
                        {% if (images) %} 
                        {% for image in images %} 
                        <li class="swiper-slide">
                           <span class="thumbnail imglink gallery_img_main">
                           <img class="main-image" src="{{ image['thumb2'] }}" title="{{ heading_title }}" alt="{{ heading_title }}" data-zoom-image="{{ image['popup'] }}" />
                           </span>
                        </li>
                        {% endfor %} 
                        {% endif %} 
                        {% if (options and theme_aurus_options_mainimg == 0) %} 
                        {% for option in options %} 
                        {% for option_value in option['product_option_value'] %} 
                        {% if (option_value['image_opt_additional'] or option_value['image']) %} 
                        <li class="swiper-slide">
                           {% if (option_value['image_opt']) %} 
                           <span class="thumbnail imglink gallery_img_main">
                           <img class="main-image" src="{{ option_value['image_opt'] }}" title="{{ heading_title }}" alt="{{ heading_title }}" data-zoom-image="{{ option_value['image_opt_popup'] }}" />
