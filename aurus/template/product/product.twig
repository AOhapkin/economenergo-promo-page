                        <!-- top -->
                        {% if (theme_aurus_sticker_top_product_auto != 1 and theme_aurus_sticker_product_top_ratinr == 1 and rating == theme_aurus_sticker_product_top_rating or theme_aurus_sticker_top_product_auto != 1 and theme_aurus_sticker_product_top_ratinr == 2 and viewed > theme_aurus_sticker_product_top_viewed) %} 
                        <div class="stickers-top"><span class="diamont_st">{{ theme_aurus_name_sticker_product_top[language_id] }}</span></div>
                        {% endif %} 
