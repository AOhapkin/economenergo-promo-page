{{ header }} 
<div class="container">
<div class="row">
{{ column_left }} 
{% if (column_left) %} 
{% set class = 'fix_cl_pr col-md-9' %} 
{% else %} 
{% set class = 'col-sm-12' %} 
{% endif %} 
<div id="cont" class="{{ class }}" itemscope itemtype="http://schema.org/Product">
<div class="col-xs-12">
   <div class="bread_scroll">
<ul class="breadcrumb" itemscope itemtype="http://schema.org/BreadcrumbList">
{% for i,breadcrumb in breadcrumbs %}
	{% if (i == 0) %}
		<li class="tab_mob_grandchild"><a href="{{ breadcrumb['href'] }}" class="current-link">{{ breadcrumb['text'] }}</a></li>
	{% elseif (breadcrumbs|length > i+1 ) %}
		<li class="tab_mob_grandchild" itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem">
			<a href="{{ breadcrumb['href'] }}" itemprop="item" itemscope itemtype="https://schema.org/Thing" itemid="{{ breadcrumb['href'] }}" title="{{ breadcrumb['text'] }}"><span itemprop="name">{{ breadcrumb['text'] }}</span></a><meta itemprop="position" content="{{ i }}">
		</li>
	{% else %}
		<li class="tab_mob_grandchild" itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem">
			<span itemprop="item" itemscope itemtype="https://schema.org/Thing" itemid="{{ breadcrumb['href'] }}" title="{{ breadcrumb['text'] }}"><span itemprop="name">{{ breadcrumb['text'] }}</span></span><meta itemprop="position" content="{{ i }}">
		</li>
	{% endif %}
{% endfor %}
</ul>
   </div>
   <h1 class="pr_h1" itemprop="name">{{ heading_title }}</h1>
   {% if (theme_aurus_st_cart != 1 ) %}  
   <div class="st_cart">
      <div class="col-xs-12">
         <div class="hidden-xs hidden-sm col-md-1">
            <img class="img_bot img-responsive" src="{{ thumb_clone }}" title="{{ heading_title }}" alt="{{ heading_title }}">
         </div>
         <div class="title_bot hidden-xs col-xs-5 col-sm-4 col-md-4">
            {{ heading_title }} 
         </div>
         <div class="price_bot col-xs-6 col-sm-3 col-md-3">
            {% if (not special) %} 
            <span class="price-new">{{ price }}</span>
            {% else %} 
            <span class="price-new">{{ special }}</span><span class="price-old">{{ price }}</span>
            {% endif %} 
         </div>
         <div class="cart_bot col-xs-6 col-sm-5 col-md-4">
            {% if (quantity <= 0 and aridiusinstock_status ) %} 
            <button type="button" class="btn-instock" onclick="instock.add('{{ product_id }}');" data-product-id="{{ product_id }}">{{ button_instock }}</button>
            {% else %} 
            <button type="button" id="button-cart_st2" data-loading-text="{{ text_loading }}" class="btn-cart product"><i class="fa fa-shopping-cart hidden-xs"></i> {{ button_cart }}</button>
            {% if (aridiusfastorder_status) %} 
            <button type="button" id="fast-order3" class="btn-cart"><i class="fa fa-clock-o hidden-xs"></i> {{ button_titlef }}</button>
            {% endif %} 
            {% endif %} 
         </div>
      </div>
   </div>
   {% endif %} 
   {% if (review_status and theme_aurus_rating_pr != 1) %} 
   <div class="linetoppr">
      <div class="rating">
         <p>
            {% set i = 1 %}{% for i in i..5 %} 
            {% if (rating < i) %} 
            <span class="fa fa-stack"><i class="fa fa-star-o fa-stack-1x"></i></span>
            {% else %} 
            <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i><i class="fa fa-star-o fa-stack-1x"></i></span>
            {% endif %} 
            {% endfor %} 
            <a class="review_profile" style="padding-bottom: 4px;" href="" onclick="$('a[href=\'#tab-review\']').trigger('click'); return false;">&nbsp;&nbsp;{{ reviews }}</a> <span>/</span> <a class="review_profile" href="" onclick="$('a[href=\'#tab-review\']').trigger('click'); return false;">{{ text_write }}</a>
            {% if (rating) %} 
            <span itemprop="aggregateRating" itemscope itemtype = "http://schema.org/AggregateRating">
               <meta itemprop='ratingValue' content='{{ rating }}' />
               <meta itemprop='bestRating' content='5' />
               <meta itemprop='worstRating' content='1' />
               <meta itemprop="reviewCount" content="{{ sh_reviews }}" />
            </span>
            {% endif %} 
         </p>
      </div>
   </div>
   {% endif %} 
   <div id="nav_pr">
      <ul class="nav_pr nav nav-tabs mod">
         <li class="active">
            <a href="#tab-product" data-toggle="tab">
            {{ text_tabb_main }} 
            </a>
         </li>
         {% if (attribute_groups) %} 
         <li>
            <a href="#tab-specification" data-toggle="tab">
            {{ tab_attribute }} 
            </a>
         </li>
         {% endif %} 
         {% if (images and theme_aurus_show_tab_img != 1) %}			 
         <li>
            <a href="#tab-image" data-toggle="tab">
            {{ tab_images }} 
            </a>
         </li>
         {% endif %}	 
         {% if (review_status) %} 
         <li>
            <a href="#tab-review" data-toggle="tab">
            {{ tab_review }} 
            </a>
         </li>
         {% endif %} 
         {% if (theme_aurus_relpr_tab == 1 and products) %} 
         <li>
            <a href="#tab-relpr" data-toggle="tab">
            {{ text_related2 }} 
            </a>
         </li>
         {% endif %} 
		 
		 
         {% if (custom_tabs) %} 
         {% for key,custom_tab in custom_tabs %} 
         {% if (custom_tab['title']) %} 
         <li>
            <a href="#tab-custom-{{ key }}" data-toggle="tab">
            {{ custom_tab['title'] }} 
            </a>
         </li>
         {% endif %} 
         {% endfor %} 
         {% endif %} 
		 
		 

		      
			  <!-- theme_tabs -->
               {% if (theme_tabs ) %}
               {% for key, theme_tab  in theme_tabs %}
               {% if (theme_tab.title[language_id]) %}
         <li>
            <a href="#tab-add-{{ key }}" data-toggle="tab">
        {{ theme_tab.title[language_id] }}
            </a>
         </li>
               {% endif %}
               {% endfor %}
               {% endif %}
               <!-- theme_tabs -->
		 
		 
      </ul>
   </div>
</div>
{% if (column_left) %} 
<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
{% else %} 
<div class="change_col col-xs-12">
